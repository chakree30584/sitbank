<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Main</servlet-name>
        <servlet-class>sit.bank.servlet.Main</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchAjaxServlet</servlet-name>
        <servlet-class>sit.bank.servlet.SearchAjaxServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ThaiBahtAjaxServlet</servlet-name>
        <servlet-class>sit.bank.servlet.ThaiBahtAjaxServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AccountCmdServlet</servlet-name>
        <servlet-class>sit.bank.servlet.AccountCmdServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Main</servlet-name>
        <url-pattern>/Main</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchAjaxServlet</servlet-name>
        <url-pattern>/SearchAjaxServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ThaiBahtAjaxServlet</servlet-name>
        <url-pattern>/ThaiBahtAjaxServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AccountCmdServlet</servlet-name>
        <url-pattern>/AccountCmdServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
